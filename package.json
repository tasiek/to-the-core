{
	"name": "to-the-core",
	"version": "0.0.1",
	"description": "Phaser3 game",
	"scripts": {
		"start": "parcel src/index.html -p 8800",
		"build": "parcel build src/index.html --out-dir dist --public-url https://tasiek.github.io/to-the-core/",
		"test": "echo \"Error: no test specified\" && exit 1",
		"lint": "eslint ./src --ext .js,.jsx,.ts,.tsx",
		"dist": "git subtree push --prefix dist origin dev-dist",
		"publish": "npm run build && npm run push-dist"
	},
	"author": "supertommy",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/tasiek/to-the-core.git"
	},
	"homepage": "https://github.com/tasiek/to-the-core",
	"devDependencies": {
		"@typescript-eslint/eslint-plugin": "^2.34.0",
		"@typescript-eslint/parser": "^2.34.0",
		"eslint": "^6.8.0",
		"minimist": ">=1.2.2",
		"parcel-plugin-clean-easy": "^1.0.2",
		"parcel-plugin-static-files-copy": "^2.5.0",
		"phaser3-rex-plugins-types": "0.0.3",
		"typescript": "^3.9.7"
	},
	"dependencies": {
		"parcel": "^1.12.4",
		"phaser": "^3.50.1",
		"phaser3-rex-plugins": "^1.1.35"
	},
	"parcelCleanPaths": [
		"dist"
	],
	"staticFiles": {
		"staticPath": "public",
		"watcherGlob": "**"
	}
}
